source 'https://github.com/CocoaPods/Specs.git'
project 'ParaClient.xcodeproj'
use_frameworks!

def para_pods
    pod 'CryptoSwift', '~> 0.5.2'
    pod 'SwiftyJSON', '~> 2.4'
    pod 'Alamofire', '~> 3.5'
end

target "ParaClient iOS" do
    para_pods
end
target "ParaClient tvOS" do
    para_pods
end
target "ParaClient OSX" do
    para_pods
end
target "ParaClient watchOS" do
    para_pods
end
target "ParaClientTests" do
    para_pods
end
target "ParaClientExample" do
	para_pods
end
